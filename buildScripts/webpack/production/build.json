{
    "bodyTag"    : "<body>",
    "buildFolder": "dist/production",
    "environment": "production",
    "mainInput": "./src/Main.mjs",
    "mainOutput": "main.js",

    "workers": {
        "data": {
            "input": "./src/worker/Data.mjs",
            "output": "dataworker.js"
        },
        "vdom": {
            "input": "./src/worker/VDom.mjs",
            "output": "vdomworker.js"
        }
    },

    "examples": {
        "button": {
            "input": "examples/component/Button.mjs",
            "output": "/examples/component/button/",
            "title" : "Neo Button"
        },
        "chip": {
            "input": "examples/component/Chip.mjs",
            "output": "/examples/component/chip/",
            "title" : "Neo Chip"
        },
        "chipfield": {
            "input": "examples/form/field/Chip.mjs",
            "output": "/examples/form/field/chip/",
            "title" : "Neo ChipField"
        },
        "circle": {
            "input": "examples/component/Circle.mjs",
            "output": "/examples/component/circle/",
            "title" : "Neo Circle"
        },
        "container": {
            "bodyTag": "<body style='margin: 10px'><p>Container Demo</p><div id='main-container'></div>",
            "input": "examples/Container.mjs",
            "output": "/examples/container/",
            "title" : "Neo Containers"
        },
        "coronaGallery": {
            "input": "examples/component/CoronaGallery.mjs",
            "output": "/examples/component/coronaGallery/",
            "themes": "'neo-theme-dark'",
            "title" : "COVID19 Gallery"
        },
        "coronaHelix": {
            "input": "examples/component/CoronaHelix.mjs",
            "output": "/examples/component/coronaHelix/",
            "themes": "'neo-theme-dark'",
            "title" : "COVID19 Helix"
        },
        "datefield": {
            "input": "examples/form/field/Date.mjs",
            "output": "/examples/form/field/date/",
            "title" : "Neo DateField"
        },
        "dateSelector": {
            "input": "examples/component/DateSelector.mjs",
            "output": "/examples/component/dateSelector/",
            "title" : "Neo DateSelector"
        },
        "docs": {
            "input": "docs/app.mjs",
            "mainThreadAddons": "'GoogleAnalytics', 'HighlightJS', 'Stylesheet'",
            "output": "/docs/",
            "title": "Neo Docs"
        },
        "emailfield": {
            "input": "examples/form/field/Email.mjs",
            "output": "/examples/form/field/email/",
            "title" : "Neo EmailField"
        },
        "fields": {
            "bodyTag": "<body style='margin: 10px'><p>Fields Demo</p><div id='main-container'></div>",
            "input": "examples/Fields.mjs",
            "output": "/examples/fields/",
            "title" : "Neo Fields"
        },
        "gallery": {
            "input": "examples/component/Gallery.mjs",
            "output": "/examples/component/gallery/",
            "title" : "Neo Gallery"
        },
        "helix": {
            "input": "examples/component/Helix.mjs",
            "output": "/examples/component/helix/",
            "title" : "Neo Helix"
        },
        "numberfield": {
            "input": "examples/form/field/Number.mjs",
            "output": "/examples/form/field/number/",
            "title" : "Neo NumberField"
        },
        "panel": {
            "bodyTag": "<body style='margin: 10px'><p>Panel Demo</p><div id='main-container'></div>",
            "input": "examples/Panel.mjs",
            "output": "/examples/panel/",
            "title" : "Neo Panels"
        },
        "pickerfield": {
            "input": "examples/form/field/Picker.mjs",
            "output": "/examples/form/field/picker/",
            "title" : "Neo PickerField"
        },
        "selectfield": {
            "input": "examples/form/field/Select.mjs",
            "output": "/examples/form/field/select/",
            "title" : "Neo SelectField"
        },
        "tabContainer": {
            "input": "examples/tab/Container.mjs",
            "output": "/examples/tab/container/",
            "title" : "Neo TabContainer"
        },
        "tableContainer": {
            "input": "examples/table/Container.mjs",
            "output": "/examples/table/container/",
            "title" : "Neo TableContainer"
        },
        "tablePerformance": {
            "bodyTag": "<body style='margin: 10px'><div id='main-container'></div><br><div id='main-container2'></div><br><div id='main-container3'></div>",
            "input": "examples/TablePerformance.mjs",
            "output": "/examples/tablePerformance/",
            "title" : "Neo Table Performance"
        },
        "tableStore": {
            "input": "examples/TableStore.mjs",
            "output": "/examples/tableStore/",
            "title" : "Neo Table & Store"
        },
        "tabs": {
            "bodyTag": "<body style='margin: 10px'><p>TabContainer App 1</p><div id='main-container'></div><br><p>TabContainer App 2</p><div id='main-container2'></div><br><div id='main-container3'></div>",
            "input": "examples/Tabs.mjs",
            "output": "/examples/tabs/",
            "title" : "Neo Tabs"
        },
        "textfield": {
            "input": "examples/form/field/Text.mjs",
            "output": "/examples/form/field/text/",
            "title" : "Neo TextField"
        },
        "timefield": {
            "input": "examples/form/field/Time.mjs",
            "output": "/examples/form/field/time/",
            "title" : "Neo TimeField"
        },
        "viewport": {
            "input": "examples/Viewport.mjs",
            "output": "/examples/viewport/",
            "title" : "Neo Viewport"
        },
        "window": {
            "bodyTag": "<body style='margin: 10px'><p>Neo Window</p><div id='main-container'></div>",
            "input": "examples/Window.mjs",
            "output": "/examples/window/",
            "title" : "Neo Window"
        }
    }
}